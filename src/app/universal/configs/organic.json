{
    "theme": "organic",
    "description": "Inspired by nature with earthy tones and soft shapes.",
    "categoryLayout": "tabs",
    "filterLayout": "sidebar-left",
    "gridLayout": "grid",
    "cardStyle": "blob",
    "title": "Pure & Natural",
    "subtitle": "Earth First",
    "style": {
        "colors": {
            "primary": "#416d47",
            "onPrimary": "#ffffff",
            "secondary": "#d8e2dc",
            "onSecondary": "#2f3e46",
            "background": "#f4f7f6",
            "text": "#2f3e46",
            "border": "#cad2c5",
            "cardBg": "#ffffff"
        },
        "typography": {
            "fontFamily": "Merriweather, serif",
            "headingsFamily": "Merriweather, serif"
        },
        "shape": {
            "borderRadius": "16px",
            "borderWidth": "1px"
        },
        "effects": {
            "shadow": "0 4px 6px -1px rgba(0, 0, 0, 0.05), 0 2px 4px -1px rgba(0, 0, 0, 0.03)"
        }
    },
    "animations": true,
    "apiConfig": {
        "endpoint": "products/category/skincare",
        "params": {
            "limit": 8
        }
    },
    "features": {
        "hero": false
    },
    "header": {
        "links": [
            {
                "label": "Market",
                "href": "/universal/organic"
            },
            {
                "label": "Our Story",
                "href": "/universal/organic/about"
            },
            {
                "label": "Connect",
                "href": "/universal/organic/contact"
            },
            {
                "label": "Join",
                "href": "/universal/organic/login"
            }
        ],
        "cartIcon": true
    },
    "footer": {
        "text": "Sustainably sourced. Ethically made.",
        "links": [
            {
                "label": "Privacy",
                "href": "/universal/organic/privacy"
            },
            {
                "label": "Terms",
                "href": "/universal/organic/terms"
            }
        ]
    }
}